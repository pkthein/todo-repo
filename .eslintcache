n            <TodoListings\r\n              success={{ value: 'Done' }}\r\n              successClick={ removeTask }\r\n              title={ 'Tasks' }\r\n              data={ tasks }\r\n            />\r\n          </div>\r\n          \r\n          <div style={ style.box }>\r\n            <TodoListings\r\n              danger={{ value: 'Checkout' }}\r\n              dangerClick={ removeDone }\r\n              warning={{ value: 'Undo' }}\r\n              warningClick={ moveToTasks }\r\n              title={ 'Done' }\r\n              data={ dones }\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Todo\r\n","C:\\Users\\PhyoKo\\Desktop\\github\\react-boot-intro\\src\\components\\TTT.js",["60"],"import React from 'react'\r\nconst TTT = ({}) => {\r\n  const [row1, setRow1] = React.useState(['-', '-', '-'])\r\n  const [row2, setRow2] = React.useState(['-', '-', '-'])\r\n  const [row3, setRow3] = React.useState(['-', '-', '-'])\r\n  const [turn, setTurn] = React.useState(0)\r\n  const [message, setMessage] = React.useState('')\r\n  const updateRow = (r, c) => {\r\n    let row\r\n    if (r === 1) { row = row1 } else if (r === 2) { row = row2 } else { row = row3 }\r\n    row[c] = turn % 2 ? 'X' : 'O'\r\n    setTurn(turn + 1)\r\n    if (r === 1) { setRow1(row) } else if (r === 2) { setRow2(row) } else { setRow3(row) }\r\n    evalGame(turn - 1)\r\n  }\r\n  const evalGame = (turn) => {\r\n    let flag = false\r\n    if (row1[0] === row1[1] && row1[1] === row1[2] && row1[0] !== '-') { flag = true } else if (row2[0] === row2[1] && row2[1] === row2[2] && row2[0] !== '-') { flag = true } else if (row3[0] === row3[1] && row3[1] === row3[2] && row3[0] !== '-') { flag = true } else if (row1[0] === row2[0] && row2[0] === row3[0] && row1[0] !== '-') { flag = true } else if (row1[1] === row2[1] && row2[1] === row3[1] && row1[1] !== '-') { flag = true } else if (row1[2] === row2[2] && row2[2] === row3[2] && row1[2] !== 